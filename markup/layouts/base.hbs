<!-- Doctype HTML5 -->
<!doctype html>
<html dir='ltr'>
{{#block 'head'}}
  <head>
    {{#block 'meta'}}
    {{> meta}}
    {{/block}}

    {{#block 'relationships'}}
    {{> relationships}}
    {{/block}}

    {{#block 'title'}}
    <title>{{title}}</title>
    {{/block}}

    {{#block 'styles'}}
    {{#each stylesheets}}
    <link href='{{this}}' rel='stylesheet' />
    {{/each}}
    {{/block}}

    <link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Roboto'>

  </head>
{{/block}}

{{#block 'body'}}
  <body class='{{bodyclass}}'>

    {{#block 'main'}}
    {{/block}}

    {{#block 'scripts'}}
    {{#each scripts}}
    <script defer src='{{this}}'></script>
    {{/each}}
    {{/block}}
  </body>
{{/block}}
</html>
